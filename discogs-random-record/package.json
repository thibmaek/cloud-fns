{
  "name": "discogs-random-record",
  "version": "2.0.0",
  "description": "Cloud function to get a random record from your Discogs collection",
  "author": "Thibault Maekelbergh",
  "repository": "thibmaek/cloud-fns",
  "scripts": {
    "start": "sls offline",
    "dev": "sls invoke local -f getRandomRecord",
    "build": "sls package -p dist",
    "deploy": "sls deploy"
  },
  "dependencies": {
    "discojs": "^2.0.0",
    "got": "^12.0.1",
    "pure-fun": "^2.0.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/got": "^9.6.12",
    "@types/node": "^17.0.17",
    "serverless-offline": "^8.4.0",
    "serverless-plugin-typescript": "^2.1.1"
  }
}
